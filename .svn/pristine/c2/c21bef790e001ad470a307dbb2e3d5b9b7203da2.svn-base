<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/FrameTop_TopRelative"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#1C1C1C"
        android:paddingLeft="@dimen/view_frame_topeleft" >

        <ImageView
            android:id="@+id/FrameTop_MenuImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/tab_menu" />

        <ImageView
            android:id="@+id/FrameTop_SearchImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/view_frame_topmargin"
            android:layout_toRightOf="@+id/FrameTop_MenuImg"
            android:src="@drawable/shared_searchbuttom_normal" />

        <ImageView
            android:id="@+id/FrameTop_Icon1Img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/sale_goods_tip_yohood" />

        <ImageView
            android:id="@+id/FrameTop_Icon2Img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/yoho_famile_small"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/FrameTop_SweepImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/sweep_bg" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/FrameTop_BottemRelative"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/FrameTop_HomeRelative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/FrameTop_HomeTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="首页"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/FrameTop_HomeImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/FrameTop_HomeTv"
                android:layout_centerHorizontal="true"
                android:src="@drawable/tabbar_homepage_highlighted_gray" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FrameTop_CategoryRelative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/FrameTop_CategoryTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="分类"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/FrameTop_CategoryImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/FrameTop_CategoryTv"
                android:layout_centerHorizontal="true"
                android:src="@drawable/tabbar_category_normal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FrameTop_SeeRelative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/FrameTop_SeeTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="逛"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/FrameTop_SeeImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/FrameTop_SeeTv"
                android:layout_centerHorizontal="true"
                android:src="@drawable/tabbar_see_normal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FrameTop_CartRelative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/FrameTop_CartTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="购物车"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/FrameTop_CartImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/FrameTop_CartTv"
                android:layout_centerHorizontal="true"
                android:src="@drawable/tabbar_cart_normal" />

            <TextView
                android:id="@+id/FrameTop_NumTv"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/view_frame_numright"
                android:layout_marginTop="@dimen/view_frame_num"
                android:background="@drawable/homepage_shopcart"
                android:gravity="center"
                android:text="3"
                android:textColor="#ffffff"
                android:textSize="10sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FrameTop_UserRelative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <TextView
                android:id="@+id/FrameTop_UserTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="我的"
                android:textColor="#000000"
                android:textSize="12sp" />

            <ImageView
                android:id="@+id/FrameTop_UserImg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/FrameTop_UserTv"
                android:layout_centerHorizontal="true"
                android:src="@drawable/tabbar_mine_normal" />
        </RelativeLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/FrameTop_Line"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_above="@id/FrameTop_BottemRelative"
        android:background="@drawable/lv_line" />

    <RelativeLayout
        android:id="@+id/FrameTop_ViewGroup"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/FrameTop_Line"
        android:layout_below="@id/FrameTop_TopRelative" >
    </RelativeLayout>

</RelativeLayout>